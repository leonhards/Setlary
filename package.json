{
  "name": "setlary",
  "version": "1.0.0",
  "description": "Setlary theme for setlary.com",
  "author": "Leon Sinaga",
  "license": "GPL-3.0",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@wordpress/scripts": "^30.4.0",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.3.3",
    "dir-archiver": "^2.1.0",
    "gulp": "^5.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "postcss": "^8.4.21",
    "sass": "^1.57.1"
  },
  "overrides": {
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2"
  },
  "scripts": {
    "lint:css": "wp-scripts lint-style src/sass/**/*.scss",
    "fix:css": "wp-scripts lint-style src/sass/**/*.scss --fix",
    "lint:js": "wp-scripts lint-js src/js/**/*.js",
    "fix:js": "wp-scripts lint-js src/js/**/*.js --fix",
    "lint:gulp": "wp-scripts lint-js gulpfile.js",
    "fix:gulp": "wp-scripts lint-js gulpfile.js --fix",
    "dev": "dir-archiver --src . --dest ../setlary-dev.zip --exclude .DS_Store .git composer.lock node_modules vendor package-lock.json .phpcs-cache",
    "prod": "dir-archiver --src . --dest ../setlary-prod.zip --exclude .DS_Store .git composer.lock node_modules vendor package-lock.json .stylelintrc.json .gitattributes .github .gitignore composer.json package.json phpcs.xml.dist .editorconfig .phpcs-cache"
  },
  "dependencies": {
    "@es-joy/jsdoccomment": "^0.49.0",
    "eslint-plugin-jsdoc": "^50.4.3"
  }
}
